<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap     
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"     
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="rule">


	<select id="queryRelationshipWithGroup" parameterClass="java.lang.Long"
		resultClass="java.lang.Long">
		select rule_group_id from t_gs_relation_rulegroup_rule
		where rule_id
		=#id#
	</select>


	<select id="queryRelationshipWithEvent" parameterClass="java.lang.Long"
		resultClass="java.lang.Long">
		select event_id from T_GS_RELATION_RULE_EVENT where
		rule_id
		=#id#
	</select>

</sqlMap>